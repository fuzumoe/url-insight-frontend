{
  "name": "urlinsight-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:ci": "start-server-and-test 'vite' http://localhost:5174 'cypress run'",
    "test:e2e:coverage": "start-server-and-test 'vite' http://localhost:5174 'cypress run --env coverage=true'",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css,md}",
    "format:check": "prettier --check src/**/*.{js,jsx,ts,tsx,json,css,md}",
    "prepare": "node -e \"try{require('./.husky/._/husky')}catch(e){}\"",
    "lint-staged": "lint-staged",
    "commitlint": "commitlint --edit",
    "start": "vite"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@cypress/code-coverage": "^3.14.5",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.30.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "cypress": "^14.5.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "istanbul-lib-coverage": "^3.2.2",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.2.0",
    "nyc": "^17.1.0",
    "postcss": "^8.4.31",
    "prettier": "^3.6.2",
    "start-server-and-test": "^2.0.12",
    "tailwindcss": "^3.3.5",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^5.0.10",
    "vite-plugin-istanbul": "^7.1.0",
    "vitest": "^3.2.4"
  }
}
